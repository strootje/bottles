[{
	"type": 1,
	"title": "OpenVPN Client",
	"description": "OpenVPN for Containers",
	"logo": "https://raw.githubusercontent.com/dperson/openvpn-client/master/logo.png",
	"categories": [ "github.com", "dperson", "openvpn", "vpn" ],
	"notes": "https://github.com/dperson/openvpn-client",

	"image": "dperson/openvpn-client",
	"restart_policy": "unless-stopped",
	"volumes": [
		{ "container": "/vpn" },
		{ "container": "/dev/net/tun", "bind": "/dev/net/tun" }],

	"env": [
		{
			"name": "GROUPID",
			"label": "Portainer Group ID",
			"default": "1001"
		},
		{
			"name": "TZ",
			"label": "Timezone",
			"default": "Europe/Amsterdam"
		},
		{
			"name": "FIREWALL",
			"label": "Firewall",
			"default": ""
		},
		{
			"name": "ROUTE",
			"label": "Network Route (ipv4)",
			"description": "Add a route to allow replies to your private network"
		},
		{
			"name": "ROUTE6",
			"label": "Network Route (ipv6)",
			"description": "Add a route to allow replies to your private network"
		}
	]
}]
