[
	{
		"type": 3,
		"title": "Ackee",
		"description": "Self-hosted, Node.js based analytics tool for those who care about privacy",
		"categories": [
			"ackee",
			"analytics",
			"tracking",
			"website"
		],
		"repository": {
			"url": "https://github.com/strootje/bottles",
			"stackfile": "https://raw.githubusercontent.com/strootje/bottles/master/src/portainer/com.electerious.Ackee/docker-compose.yaml"
		},
		"env": [
			{
				"name": "VIRTUAL_HOST",
				"label": "Virtual host name",
				"default": "ackee.local"
			},
			{
				"name": "VIRTUAL_PORT",
				"label": "Virtual host port",
				"default": "3000",
				"preset": true
			},
			{
				"name": "LETSENCRYPT_HOST",
				"label": "Let's Encrypt Host",
				"default": "ackee.local"
			},
			{
				"name": "ACKEE_VOLUMES_DIR",
				"label": "Ackee Volumes Folder",
				"default": "/opt/docker/ackee/ackee"
			},
			{
				"name": "ACKEE_USERNAME",
				"label": "Ackee Username"
			},
			{
				"name": "ACKEE_PASSWORD",
				"label": "Ackee Password"
			}
		]
	}
]
