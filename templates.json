	{
		"type": 3,
		"title": "Let's Encrypt",
		"description": "Reverse SLL Proxy",
		"logo": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/le-logo-wide.png",

		"repository": {
			"url": "https://github.com/strootje/bottles.git",
			"stackfile": "letsencrypt/docker-compose.yaml"
		},

		"env": [
			{
				"name": "PUID",
				"label": "Portainer User ID"
			},
			{
				"name": "PGID",
				"label": "Portainer Group ID"
			},
			{
				"name": "TZ",
				"label": "Timezone",
